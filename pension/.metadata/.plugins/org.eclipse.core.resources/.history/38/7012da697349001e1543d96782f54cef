var id = document.getElementById("id");
var name = document.getElementById("name");
var pw = document.getElementById("pw");
var pw_ck = document.getElementById("pw_ck");
var email = document.getElementById("email");
var tel = document.getElementById("tel");
var code = document.getElementById("code");
var code_ck = document.getElementById("code_ck");
function join1(){
	if(f.id.value==""){
		alert("아이디를 입력하세요");
	}
	else if(f.id.value.length<6 || f.id.value.length>16){
		alert("아이디를 6~16자로 입력");
	}
	else if(f.name.value==""){
		alert("이름을 입력하세요");
	}
	else if(f.pw.value==""){
		alert("비밀번호 입력");
	}
	else if(f.pw.value.length<6 || f.pw.value.length>12){
		alert("비밀 번호를 6~12자리로 입력");
	}	
	else if(f.pw_ck.value!=f.pw.value){
		alert("비밀번호 불일치");
	}
	else if(f.email.value==""){
		alert("이메일 입력하세요");
	}
	else if(f.email.value.indexOf('@') == -1 ){
		alert("정상적인 이메일 형식 입력바람");
	}
	else if(f.tel.value==""){
		alert("전화 번호를 입력하세요");
	}
	else if(f.tel.value.length<10 || f.tel.value.length>11){
		alert("전화번호는 10~11자리 입력바람");
	}
	else{
		f.submit();
	}
}

