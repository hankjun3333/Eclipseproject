package array_test;

public class array2 {

	public static void main(String[] args) {
		new array2().test();
		
	}
	public void test() {
		/*
		 응용문제 1번 
		 해당 사용자 아이디 배열값 중 아이디가 3자리 이하의 사용자만 출력하시오 
		 */
		String id[] = {"hong","kim","park","lee","jang","so"};
		int w = 0;
		int ea = id.length;
		/*
		 * for(w=0; w<ea; w++) { //System.out.println(id[w]); if(id[w].length() <= 3) {
		 * System.out.println(id[w]); } }
		 */
		while(w<ea) {
			System.out.println(id[w]);
			w++;
		}
	}
	
	
}
