package array_test;
//2차 배열
//정적배열은 자리만선언
//동적배열은 자리선언과 동시에 값도집어넣음
public class array6 {

	public static void main(String[] args) {
		new array6().test();
	}
	public void test() {
		//2차 배열의 기본적인 형태
		String member[][] = {
				{"박초롱","윤보미","정은지","김남주","오하영"},
				{"31","29","33","31","32"}
		};
		int ea = member.length;
		System.out.println(ea);
		int ea2 = member[5].length;
		System.out.println(ea2);
	}

}
