package array_test;
//foreach문 사용
public class array3 {

	public static void main(String[] args) {
		new array3().test();
	}
	/*
	  여기서 가장 중요으한 꿀팁! for each문 에서는 배열이 String이면
	  foreach문에서도 String을 반드시써야한다!!!!!!!!!!
	 */
	public void test() {
		String user[] = {"홍길동","김유신","장보고","유관순"};
		for(String i : user) {
			System.out.println(i);
		}
	}

}
