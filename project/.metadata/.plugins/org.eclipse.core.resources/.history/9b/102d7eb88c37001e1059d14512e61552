package array_test;

//2차 배열을
public class array7 {

	public static void main(String[] args) {
		new array7().test();
	}
	//2차 배열을 1차 배열로 만들어보자
	public void test() {
		int a[][] = {
				{1,3,5,7},
				{2,4,6,8}
		};
		int ea1 = a.length;
		System.out.println(ea1);
		int ea2 = a[0].length;
		System.out.println(ea2);
	}

}
