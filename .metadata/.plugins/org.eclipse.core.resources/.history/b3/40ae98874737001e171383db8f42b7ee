package array_test;
//2차 배열
//정적배열,동적배열 {"홍길동"}
public class array6 {

	public static void main(String[] args) {
		new array6().test();
	}
	public void test() {
		//2차배열형태의 기본 반복문이 더블반복문임
		String member[][] = {
				{"박초롱","윤보미","정은지","김남주","오하영"},
				{"31","29","33","31","32"}
		};
		int ea = member.length;
		System.out.println(ea);
		int ea1 = member[0].length;
		System.out.println(ea1);
		//밖에 for문 2번 안에 5번 이중포문임 값이 없으면 null 갯수는 맞춰줘야됨
		
		
		/*
		 * 응용문제 7 번 
		 * 다음 코드를 활용하여 예시처럼 결과값을 출력되도록 해보시오
		 * 박초롱
		 * 31
		 * 윤보미
		 * 29
		 * 정은지
		 * 31 이런식
		 * */
		int f, ff;
		for(f=0; f<ea; f++) {
			for(ff=0; ff<ea1; ff++) {
				System.out.println(member[f][ff]);
				ff++;
			}
			
		}
		
	}

}
