package array_test;
//2차 배열
//정적배열,동적배열 {"홍길동"}
public class array6 {

	public static void main(String[] args) {
		new array6().test();
	}
	public void test() {
		//2차배열형태의 기본 반복문이 더블반복문임
		String member[][] = {
				{"박초롱","윤보미","정은지","김남주","오하영"},
				{"31","29","33","31","32"}
		};
		int ea = member.length;
		System.out.println(ea);
		int ea1 = member[0].length;
		System.out.println(ea1);
		//밖에 for문 2번 안에 5번 이중포문임 값이 없으면 null 갯수는 맞춰줘야됨
		
		int f, ff;
		for(f=0; f<ea; f++) {
			for(ff=0; ff<ea1; f++) {
				System.out.println(member[f][ff]);
			}
		}
		
	}

}
