package array_test;

//foreach문 사용 : 무조건 배열에만 사용하는 반복문임 자바 8번대에 튀어나온놈
public class array3 {

	public static void main(String[] args) {
		//실행하자마자 test()실행
		new array3().test();

	}
	//for each문에서 string형이면 다 string int는 다 int
	public void test() {
		String user[] = {"홍길동","김유신","장보고","유관순"};
		
		for(String f : user) {
			System.out.println(f);
		}
	}
}
