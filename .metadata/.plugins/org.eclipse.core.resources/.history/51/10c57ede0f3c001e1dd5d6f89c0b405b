package io;

import java.io.InputStream;
import java.io.OutputStream;

//InputStream + OutputStream + flush 사용법
public class file10 {

	public static void main(String[] args) throws Exception{
		InputStream in = System.in;
		OutputStream os = System.out;
		
		int a = in.read();
		os.write(a);

	}

}
