<script>
	function abc(){
		if(f.uname.value==""){
			alert("이름을 입력하세요");
			return false;
		}
		else if(f.uid.value==""){
			alert("아이디를 입력하세요");
			return false;
		}
		else{
			return;
		}
	}
</script>
